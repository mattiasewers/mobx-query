{
  "name": "mobx-query",
  "version": "0.3.4",
  "description": "Generate mobx models based on a model and action schema",
  "main": "./mobx.js",
  "types": "./mobx.d.ts",
  "bin": {
    "mobx-query": "bin/cli"
  },
  "scripts": {
    "format": "prettier --write **/*.{js,ts,tsx,json}",
    "build": "./build_lib.sh",
    "prepublishOnly": "yarn test && yarn build",
    "test": "./bin/cli scaffold --force --test --out tests/models && jest",
    "generate": "yarn peggy --plugin ts-pegjs --extra-options-file pegconfig.json -o src/utils/parser.ts --cache src/utils/parser.pegjs"
  },
  "files": [
    "mobx.js",
    "mobx.d.ts",
    "build",
    "dist",
    "LICENSE",
    "readme.md",
    "bin"
  ],
  "license": "MIT",
  "dependencies": {
    "gluegun": "latest",
    "peggy": "^1.2.0",
    "ts-pegjs": "^1.1.1"
  },
  "peerDependencies": {
    "mobx": "^6.2.0",
    "mobx-react": "^7.1.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.2.3",
    "@testing-library/react": "^11.2.2",
    "@types/jest": "^26.0.23",
    "@types/node": "^12.7.11",
    "@types/react": "^17.0.2",
    "jest": "^27.0.5",
    "mobx": "^6.3.2",
    "mobx-react": "^7.1.0",
    "prettier": "^2.3.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-error-boundary": "^3.1.0",
    "rollup": "^2.53.2",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "tslib": "^2.3.0",
    "typescript": "^4.3.4"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  }
}
